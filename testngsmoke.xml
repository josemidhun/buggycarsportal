<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke Tests Buggy Cars Portal Tests" verbose="1" >
    <test name="Buggy Cars Cross Browser Test Edge" >
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="com.buggy.car.portals.tests.ParameterCrossBrowserTest" />
        </classes>
    </test>
    <test name="Buggy Cars Cross Browser Test Chrome" >
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.buggy.car.portals.tests.ParameterCrossBrowserTest" />
        </classes>
    </test>
</suite>